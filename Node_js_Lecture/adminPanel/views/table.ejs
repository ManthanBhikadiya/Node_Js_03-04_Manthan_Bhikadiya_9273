<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>

<body>
  <%- include('layouts/header') %>
    <table>
      <tr>
        <th>Name</th>
        <th>Email</th>
      </tr>
      <% users.forEach(user=> { %>
        <tr>
          <td>
            <%= user.name %>
          </td>
          <td>
            <%= user.email %>
          </td>
        </tr>
        <% }) %>
    </table>
    <%- include('layouts/footer') %>
</body>

</html>